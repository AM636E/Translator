@{
    List<TranslatorEntities.User> users = ViewBag.Users;
}

<div class="panel panel-default">
    <div class="panel-header">
        <h4 class="heading">
            Users
        </h4>
    </div>
    <div class="panel-body">
        <table class="table">
            <thead>
                <tr>
                    <td>
                        Id
                    </td>
                    <td>
                        Login
                    </td>
                    <td>
                        Email
                    </td>
                    <td></td>
                </tr>
            </thead>
            <tbody>
                @foreach (var user in users)
                {
                    if (!user.Login.Equals("admin"))
                    {
                        <tr>
                            <td>
                                @user.UserId
                            </td>
                            <td>
                                @user.Login
                            </td>
                            <td>
                                @user.Email
                            </td>
                            <td>
                                <a href="/admin/user/@user.UserId/delete">Delete</a>
                            </td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
</div>
